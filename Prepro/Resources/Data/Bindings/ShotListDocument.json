{
	"grouped": true,
    "sections": [
                 {
                 
                 "title" : "Shot", "bind":"title:title", "elements":
                 [
                  {
                    "type":"PPPhotoPickerElement", "key":"photo", "title":"Photo", "bind":"dataValue:photo"
                  },
                  {
                    "type":"QDecimalElement", "key":"number", "title":"Number", "fractionDigits":0, "bind":"floatValue:number"
                  },
                  {
                    "type":"QEntryElement", "key":"title", "title":"Title", "bind":"textValue:title"
                  },
                  {
                    "type":"QRadioElement",
                    "key":"type",
                    "title":"Type",
                    "items":["EWS", "VWS", "WS", "MS", "MCU", "CU", "ECU", "CA", "CI", "TS", "OTS", "POV", "DUT"],
                    "bind":"selectedItem:type"
                  },
                  {
                    "type":"QRadioElement",
                    "key": "locationType",
                    "title":"Location Type",
                    "items":["EXT", "INT", "INT/EXT", "EXT/INT", "EST"],
                    "bind": "selectedItem:locationType",
                  },
                  {
                  "type":"QRadioElement",
                  "key":"angle",
                  "title":"Angle",
                  "items":["Birds Eye view", "High", "Eye Level", "Low", "Oblique/Canted"],
                  "bind":"selectedItem:angle"
                  },
                  {
                  "type":"QRadioElement",
                  "key":"transition",
                  "title":"Transition",
                  "items":["Cut", "Straight Cut", "Contrast Cut", "L Cut", "Form Cut", "Match Cut", "Parallel Editing Cut", "Jump Cut", "Fade In / Out", "Dissolve", "Wipe", "Morph", "Other"],
                  "bind":"selectedItem:transition"
                  },
                  {
                    "type":"PPTimeCodeElement", "key":"duration", "title":"Duration", "bind":"textValue:duration"
                  },
                  {
                    "type":"QMultilineElement", "key":"descriptionText", "title":"Description", "bind":"textValue:descriptionText"
                  }
                ]
            }
     ]
}