{
	"grouped": true,
	"sections": [
        {
             
             "title" : "Cast Member", "bind":"title:name", "elements":
             [
                {
                    "type":"PPPhotoPickerElement", "key":"photo", "title":"Photo", "bind":"dataValue:photo"
                },
                {
                    "type":"QEntryElement", "key":"name", "title":"Name", "bind":"textValue:name"
                },
                {
                  "type":"QRadioElement",
                  "key": "roleType",
                  "title":"Role Type",
                  "items":["Cast", "Extra", "Stunt", "Voice"],
                  "bind" : "selectedItem:roleType"
                },
                {
                  "type":"QRadioElement",
                  "key": "status",
                  "title":"Status",
                  "items":["Approved To Play", "Approved To Voice", "Call For Rehearsal", "Call For Audition", "Shortlisted", "Dropped", "Backup"],
                  "bind" : "selectedItem:status"
                },
                {
                    "type":"QDecimalElement", "key":"idNo", "title":"ID No.", "bind":"floatValue:idNo", "fractionDigits":0
                },
                {
                    "type":"QEntryElement", "key":"characterName", "title":"Character Name", "bind":"textValue:characterName"
                },
                {
                    "type":"QEntryElement", "key":"agency", "title":"Agency", "bind":"textValue:agency"
                },
                {
                    "type":"QEntryElement", "key":"email", "title":"Email", "bind":"textValue:email"
                },
                {
                    "type":"QEntryElement", "key":"mobile", "title":"Mobile", "bind":"textValue:mobile"
                },
                {
                    "type":"QRadioElement",
                    "key": "payrate",
                    "title":"Rate",
                    "items":["N/A", "Expenses", "Flat", "Per Hour", "Per Day", "Per Week"],
                    "bind" : "selectedItem:payrate"
                },
                {
                    "type":"QDecimalElement", "key":"pay", "title":"Pay", "floatValue":0, "fractionDigits":2, "bind":"floatValue:pay"
                },
                {
                    "type":"QLabelElement", "title":"Appearance", "key":"appearance", "controllerAction" : "subSectionElementPressed:"
                }
             ]
        }
    ]
}